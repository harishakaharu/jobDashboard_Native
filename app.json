{
  "expo": {
    "name": "jobDashboard_native",
    "slug": "jobdashboardnative",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "harishakahar",
    "icon": "./assets/images/icon.png",
    "scheme": "jobdashboardnative",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "d3a6423f-4112-494a-8296-912d3a649704"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/d3a6423f-4112-494a-8296-912d3a649704"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
